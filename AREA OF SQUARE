/*
BASIC PROGRAM -13
AREA
WAP TO FIND AREA OF SQUARE
 */

import java.util.*;
public class program13 {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        System.out.println("Enter Side");
        int s= sc.nextInt();
        int area=s*s;
        System.out.println("Area of square="+area);
    }
}
