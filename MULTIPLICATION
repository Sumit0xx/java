/*
 BASIC PROGRAM -7
MULTIPLICATION " * "
 */
package basicprograms;

import java.util.*;
public class program7 {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        System.out.println("Enter a number");
        int a= sc.nextInt();
        int b=sc.nextInt();
        int c=a*b;
        System.out.println("Multiplication="+c);
    }
}
